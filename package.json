{
  "name": "@haosama/sp-ui",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "main": "./dist/umd/sp-ui.umd.cjs",
  "module": "./dist/es/sp-ui.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/sp-ui.js",
      "require": "./dist/umd/sp-ui.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-s clean build:vite:umd build:vite:es move-style",
    "build:esm": "rollup --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "build:vite:umd": "vite build --config ./vite.config.umd.ts",
    "build:vite:es": "vite build --config ./vite.config.es.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "clean": "rimraf dist",
    "release": "release-it",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "release-it": "^17.3.0",
    "rimraf": "^5.0.7",
    "rollup-plugin-bundle-scss": "^0.1.3",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.71.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^1.8.27"
  }
}
